
type PushArgs = record {
    blob: blob;
};

type PushResult = variant {
    Ok;
    Err: text;
};

type FetchArgs = record {
    oid: text;
};

type FetchResult = variant {
    Ok: blob;
    Err: text;
};

service : {
    "push": (PushArgs) -> (PushResult);
    "fetch": (FetchArgs) -> (FetchResult) query;
}